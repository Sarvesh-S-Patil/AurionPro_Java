package com.apro.command.model;

public class Radio implements Device {
	Device device;
	

	public Radio(Device device) {
		super();
		this.device = device;
	}

	@Override
	public void turnOn() {
		// TODO Auto-generated method stub
		device.turnOn();
		
	}

	@Override
	public void turnOff() {
		// TODO Auto-generated method stub
		device.turnOff();
		
	}
	

}
