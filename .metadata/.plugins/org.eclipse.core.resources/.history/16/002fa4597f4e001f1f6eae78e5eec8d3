				new Country("CA", "Canada", 2),
				new Country("CN", "China", 3),
				new Country("IN", "India", 3),
				new Country("AU", "Australia", 3),
				new Country("ZW", "Zimbabwe",4)	
				);
		
		String str = "            hello      ";
		System.out.println(str.trim());
		try {
