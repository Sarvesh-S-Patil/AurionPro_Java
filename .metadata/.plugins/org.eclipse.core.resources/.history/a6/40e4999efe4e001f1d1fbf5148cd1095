
package com.apro.creational.factory.test;

import com.apro.creational.factory.model.ICar;
import com.apro.creational.factory.model.MahindraCar;
import com.apro.creational.factory.model.MarutiCar;
import com.apro.creational.factory.model.Tata;

public class CarTest {
	public static void main(String[] args) {
		ICar marutiCar = new MarutiCar();
		ICar tataCar = new Tata();
		ICar mahindraCar = new MahindraCar();
		marutiCar.start();
		tataCar.start();
		mahindraCar.start();
		
	}

}
