package com.dip.solution.model;

public class TaxCalculator {
	
	ErrorLogger logger;

	
	
	public TaxCalculator(ErrorLogger logger) {
		super();
		this.logger = logger;
	}



	public int taxCalculator(int amount,int rate) {
		int tax =0;
		try {
			tax = amount/rate;
		}
		catch(Exception exception) {
				logger = new FIleLogger();
				logger.error("Divide by zero");
				logger = new DBLogger();
				logger.error("Divide by zero");
		}
		return tax;
	}
	

}
