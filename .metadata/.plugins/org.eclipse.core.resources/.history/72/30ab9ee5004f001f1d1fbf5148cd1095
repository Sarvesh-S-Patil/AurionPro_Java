package com.apro.creational.factory.model;

public class CarFactory {
	
	
	public static ICar makeCar(String carName) {
		ICar car=null;
		if(carName=="Maruti")
			return car=new MarutiCar();
		if(carName=="Tata")
			return car =new TataCar();
		if(carName=="Mahindra")
			return car=new MahindraCar();
		return car;
	}

}
