package com.apro.builder.model;

public class Account {
	long accountNumber;
	String accountName;
	int accountBalance;
	String address; //optional
	
	
	public Account(long accountNumber, String accountName, int accountBalance, String address) {
		super();
		this.accountNumber = accountNumber;
		this.accountName = accountName;
		this.accountBalance = accountBalance;
		this.address = address;  //optional
	}
	
	public static class AccountBuilder() {
		long accountNumber;
		String accountName;
		int accountBalance;
		String address;
	}

	@Override
	public String toString() {
		return "Account [accountNumber=" + accountNumber + ", accountName=" + accountName + ", accountBalance="
				+ accountBalance + ", address=" + address + "]";
	}
	
	

}
